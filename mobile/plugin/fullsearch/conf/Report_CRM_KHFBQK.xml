<?xml version="1.0" encoding="UTF-8"?>
<!--  -->
<workflow workflowId="">
	<!-- name. 传递的参数,name不能修改.   column="CrossDomain" 表示跨域.默认空  url对应的defaultV 是ajax请求页面 -->
	<field name="url" column="" defaultV="/mobile/plugin/crm_new/CRMReportData.jsp" defaultN="客户分布情况"></field>
	
	
	<!--直接包含返回的json中. 返回值判断,默认空.即为msg. 0标识成功. -->
	<field name="return_flag" column="SuccessF" defaultV="msg" defaultN="成功标识"></field>
	<field name="return_flag" column="SuccessV" defaultV="true" defaultN="成功值"></field>
	<!-- 错误信息显示 -->
	<field name="return_flag" column="ErrMsg" defaultV="" defaultN="错误信息"></field>
	<!--返回list对应的key值. -->
	<field name="return_flag" column="RetList" defaultV="datas" defaultN="返回列表key值"></field>
	<!--默认,空.返回示例如下图.标准返回 空或者1: -->
	<field name="return_flag" column="RetType" defaultV="3" defaultN="返回类型"></field>
	<!-- 数据显示格式默认1 -->
	<field name="return_flag" column="ShowStyle" defaultV="5" defaultN="列表展示样式"></field>
	<!-- 每条数据是否显示序号(1开始),类似排名 -->
	<field name="return_flag" column="DisplayRanking" defaultV="0" defaultN="是否显示排名"></field>
	<!-- 每条数据是否小图片,默认未空.不需要.defaultV 可以设置默认图标  -->
	<field name="return_flag" column="DisplayIcon" defaultV="0" defaultN="显示小图标"></field>
	
	<!-- 返回统计值 -->
	<field name="return_statistics" column="sumcount" defaultN="客户总数：{sumcount}" bgColor="#009ffb"></field>
	<field name="return_statistics" column="provinceNum,cityNum" defaultN="覆盖全国{provinceNum}个省份，{cityNum}个城市" bgColor="#726bff"></field>
	
	<field name="return_" column="province" defaultN="省份"></field>
	<field name="return_" column="num" defaultN="客户数"></field>
	<field name="return_" column="proportion" defaultN="占比"></field>
</workflow>