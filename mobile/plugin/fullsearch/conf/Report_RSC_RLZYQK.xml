<?xml version="1.0" encoding="UTF-8"?>
<!--  -->
<workflow workflowId="">
	<!-- name. 传递的参数,name不能修改.   column="CrossDomain" 表示跨域.默认空  url对应的defaultV 是ajax请求页面 -->
	<field name="url" column="" defaultV="/mobile/plugin/hrm/hrminterface4E.jsp?type=getAllResourceNum" defaultN="人力资源统计报表"></field>
	
	
	<!--直接包含返回的json中. 返回值判断,默认空.即为msg. 0标识成功. -->
	<field name="return_flag" column="SuccessF" defaultV="msg" defaultN="成功标识"></field>
	<field name="return_flag" column="SuccessV" defaultV="true" defaultN="成功值"></field>
	<!-- 错误信息显示 -->
	<field name="return_flag" column="ErrMsg" defaultV="" defaultN="错误信息"></field>
	<!--默认,空.返回示例如下图.标准返回 空或者1: -->
	<field name="return_flag" column="RetType" defaultV="2" defaultN="返回类型"></field>
	<!-- 数据显示格式默认1 -->
	<field name="return_flag" column="ShowStyle" defaultV="5" defaultN="列表展示样式"></field>
	<!-- 每条数据是否显示序号(1开始),类似排名 -->
	<field name="return_flag" column="DisplayRanking" defaultV="0" defaultN="是否显示排名"></field>
	<!-- 每条数据是否小图片,默认未空.不需要.defaultV 可以设置默认图标  -->
	<field name="return_flag" column="DisplayIcon" defaultV="0" defaultN="显示小图标"></field>
	
	<!-- 返回统计值 -->
	<field name="return_statistics" column="allresourcenum" defaultN="在职人数：{allresourcenum}" bgColor="#1673dd"></field>
	<field name="return_statistics" column="sex" defaultN="性别比例(男:女)：{sex}" bgColor="#6d3ed9"></field>
	<field name="return_statistics" column="avgage" defaultN="平均年龄：{avgage}" bgColor="#0ea736"></field>
	<field name="return_statistics" column="monthenter" defaultN="本月新入职人数：{monthenter}" bgColor="#db893a"></field>
	
</workflow>