<?xml version="1.0" encoding="UTF-8"?>
<!--  -->
<workflow workflowId="">
	<!-- name. 传递的参数,name不能修改.   column="CrossDomain" 表示跨域.默认空  url对应的defaultV 是ajax请求页面 -->
	<field name="url" column="" defaultV="/mobile/plugin/2/getReportForE.jsp?func=getcount" defaultN="知识文档情况"></field>
	
	<!--直接包含返回的json中. 返回值判断,默认空.即为msg. 0标识成功. -->
	<field name="return_flag" column="SuccessF" defaultV="status" defaultN="成功标识"></field>
	<field name="return_flag" column="SuccessV" defaultV="0" defaultN="成功值"></field>
	<!-- 错误信息显示 -->
	<field name="return_flag" column="ErrMsg" defaultV="" defaultN="错误信息"></field>
	<!--默认,空.返回示例如下图.标准返回 空或者1: -->
	<field name="return_flag" column="RetType" defaultV="2" defaultN="返回类型"></field>
	<!-- 数据显示格式默认1 -->
	<field name="return_flag" column="ShowStyle" defaultV="5" defaultN="展示样式"></field>
	
	<field name="return_flag" column="DisplayStr" defaultV="为您找到知识报表" defaultN="有值个性化输出"></field>
	 
	<field name="return_statistics" column="docCount" defaultN="系统中的知识文档数量：{docCount}" bgColor="#009ffb"></field>
	<field name="return_statistics" column="folderCount" defaultN="系统中的目录数量：{folderCount}" bgColor="#726bff"></field>
	
</workflow>