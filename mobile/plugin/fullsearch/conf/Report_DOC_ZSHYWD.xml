<?xml version="1.0" encoding="UTF-8"?>
<!--  -->
<workflow workflowId="">
	<!-- name. 传递的参数,name不能修改.   column="CrossDomain" 表示跨域.默认空  url对应的defaultV 是ajax请求页面 -->
	<field name="url" column="" defaultV="/mobile/plugin/2/getReportForE.jsp?func=read" defaultN="最受欢迎文档"></field>
	 
	
	
	<!--直接包含返回的json中. 返回值判断,默认空.即为msg. 0标识成功. -->
	<field name="return_flag" column="SuccessF" defaultV="status" defaultN="成功标识"></field>
	<field name="return_flag" column="SuccessV" defaultV="0" defaultN="成功值"></field>
	<!-- 错误信息显示 -->
	<field name="return_flag" column="ErrMsg" defaultV="" defaultN="错误信息"></field>
	<!--默认,空.返回示例如下图.标准返回 空或者1: -->
	<field name="return_flag" column="RetType" defaultV="3" defaultN="返回类型"></field>
	<!--返回list对应的key值.默认key值是list -->
	<field name="return_flag" column="RetList" defaultV="docList" defaultN="返回列表key值"></field>
	<!-- 数据显示格式默认1 -->
	<field name="return_flag" column="ShowStyle" defaultV="3" defaultN="展示样式"></field>
	<!-- 每条数据是否小图片,默认未空.不需要.defaultV 可以设置默认图标. DisplayIcon可以指定某个模块获取对应图标,也可以是url  -->
	<field name="return_flag" column="DisplayIcon" defaultV="DOC" defaultN="显示小图标"></field>
	<!-- 点击跳转URL url中的& 需要转移&amp; -->
	<field name="return_flag" column="ClickURL" defaultV="/mobile/plugin/2/view.jsp?fromVoice=true&amp;detailid={ID}" defaultN="跳转URL"></field>

	<!-- 有值显示结果上面的文字提示 -->
	<field name="return_flag" column="DisplayStr" defaultV="为您找到以下结果" defaultN="有值个性化输出"></field>
	
	<!-- 点击跳转URL url中的& 需要转移&amp; -->
	<field name="return_flag" column="ClickURL" defaultV="" defaultN="跳转URL"></field>
	
	<!-- 不需要的字段直接删除 -->

	<!-- 以下都是返回参数,针对list中返回的key值,name值固定 return_, column为返回list数据中json的key值,defaultN 为显示的标签名 ,ID和NAME 按照以下顺序. 其他可以自由排序 -->
	<field name="return_" column="docid" defaultV="ID"></field>
	<field name="return_" column="docsubject" defaultV="NAME" defaultN="名称"></field>
	<field name="return_" column="doccreaterid,doccreatedate,readcount" defaultV="{doccreaterid}#x20;#x20;{doccreatedate}#x20;#x20;阅读数：{readcount}" defaultN="描述"></field>
	
</workflow>