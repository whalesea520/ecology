  <function> 
    <!-- 登录 -->  
    <login method="post" result="json"> 
      <url>/mobile/plugin/VerifyLogin.jsp</url> 
    </login>  
    <!-- 非主插件登录 -->  
    <quicklogin method="post" result="json"> 
      <url>/mobile/plugin/VerifyQuickLogin.jsp</url> 
    </quicklogin>  
    <!-- 管理员登录 -->  
    <admin method="post" result="json"> 
