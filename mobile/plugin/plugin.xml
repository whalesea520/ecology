    </admin>  
    <!-- 验证 -->  
    <verify result="json"> 
      <url>/mobile/plugin/VerifySessionKey.jsp</url> 
    </verify>  
    <!-- 取用户 -->  
    <user result="json"> 
      <url>/mobile/plugin/GetUser.jsp</url> 
    </user>  
    <!-- 同步用户条件 -->  
    <sync method="post" result="json"> 
      <url>/mobile/plugin/SyncUser.jsp</url> 
    </sync>  
    <!-- 同步用户信息 -->  
    <syncuser method="post" result="json"> 
      <url>/mobile/plugin/SyncUserInfo.jsp</url> 
    </syncuser>  
    <!-- 取得用户数 -->  
    <usercount method="post" result="json"> 
      <url>/mobile/plugin/UserCount.jsp</url> 
    </usercount>  
    <!-- 用户设置界面 -->  
    <usersetting> 
      <url>/mobile/plugin/browser/MobileUserBrowser.jsp</url> 
    </usersetting>  
    <!-- 模块设置界面 -->  
    <modulesetting> 
      <url>/mobile/plugin/browser/MobileModuleBrowser.jsp</url> 
    </modulesetting>  
    <!-- 预下载文件(返回文件HashCode) -->  
    <predownload result="json"> 
      <url>/mobile/plugin/PreDownload.jsp</url> 
    </predownload>  
    <!-- 下载文件 -->  
    <download result="byte"> 
      <url>/mobile/plugin/Download.jsp</url> 
    </download>  
    <!-- 上传文件 -->  
    <upload method="post" result="json"> 
      <url>/mobile/plugin/Upload.jsp</url> 
    </upload>  
    <!-- 检查服务器状态 -->  
    <checkserver method="post" result="json"> 
      <url>/mobile/plugin/CheckServer.jsp</url> 
    </checkserver>  
    <!-- mobile数据迁移 -->  
    <update result="json"> 
      <url>/mobile/plugin/Update.jsp</url> 
